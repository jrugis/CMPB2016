	Mesh = {Action Potentials; Animals; Calcium Channels; Calcium Signaling; Dendrites; Gonadotropin-Releasing Hormone; Male; Mice; Mice, Inbred C57BL; Neurons; Organ Culture Techniques; Preoptic Area; Sodium Channels},
	Month = {Jan},
	Number = {1},
	Pages = {151-8},
