	Journal = {J Biomed Biotechnol},
	Journal-Full = {Journal of biomedicine \& biotechnology},
	Mesh = {Animals; Arteries; Calcium; Humans; Muscle, Smooth, Vascular; Muscle, Striated; Ryanodine Receptor Calcium Release Channel},
	Pages = {135249},
	Pmc = {PMC2793424},
	Pmid = {20029633},
	Pst = {ppublish},
	Title = {Role of ryanodine receptor subtypes in initiation and formation of calcium sparks in arterial smooth muscle: comparison with striated muscle},
	Volume = {2009},
