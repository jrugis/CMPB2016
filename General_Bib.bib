	Mesh = {Animals; Calcium; Diabetes Mellitus, Type 2; Glucose; Homeostasis; Humans; Insulin-Secreting Cells; Islets of Langerhans; Models, Biological; Models, Theoretical; Oscillometry; Time Factors},
	Pages = {261-79},
