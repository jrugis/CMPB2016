	Mesh = {Action Potentials; Animals; Calcium; Cells, Cultured; Electric Stimulation; Embryo, Mammalian; Hippocampus; Models, Neurological; Neural Inhibition; Neuronal Plasticity; Neurons; Rats; Synaptic Transmission; Time Factors},
	Month = {May},
	Number = {5},
	Pages = {2600-13},
	Pmid = {15625097},
	Pst = {ppublish},
	Title = {Calcium time course as a signal for spike-timing-dependent plasticity},
	Volume = {93},
