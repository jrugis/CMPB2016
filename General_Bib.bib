	Mesh = {Animals; Calcium; Calcium-Calmodulin-Dependent Protein Kinase Type 2; Calmodulin; Enzyme Activation; Enzyme Stability; Humans; Kinetics; Phosphoric Monoester Hydrolases; Protein Conformation; Stochastic Processes},
	Month = {Aug},
