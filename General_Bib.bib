	Journal-Full = {American journal of physiology. Cell physiology},
	Mesh = {Animals; Calcium; Calcium Channels; Calcium Signaling; Computer Simulation; Female; Humans; Membrane Potentials; Models, Biological; Models, Theoretical; Muscle Relaxation; Myocytes, Smooth Muscle; Myometrium; Myosin Light Chains; Patch-Clamp Techniques; Phosphorylation; Rats; Sodium-Calcium Exchanger; Time Factors; Uterine Contraction},
	Month = {May},
