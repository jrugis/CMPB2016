	Pmid = {22768938},
	Pst = {ppublish},
	Title = {The probability of triggering calcium puffs is linearly related to the number of inositol trisphosphate receptors in a cluster},
	Volume = {102},
	Year = {2012},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.bpj.2012.03.029}}

