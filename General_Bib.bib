	Mesh = {Animals; Calcium; Chloride Channels; Computer Simulation; Gastrointestinal Motility; Interstitial Cells of Cajal; Mice; Models, Biological; Muscle Contraction; Muscle, Smooth},
	Month = {Apr},
	Number = {8},
