	Title = {Investigation of the roles of {C}a$^{2+}$ and {InsP}$_3$ diffusion in the coordination of {C}a$^{2+}$ signals between connected hepatocytes},
	Volume = {114},
	Year = {2001}}

@article{clapham2007,
	Abstract = {Calcium ions (Ca(2+)) impact nearly every aspect of cellular life. This review examines the principles of Ca(2+) signaling, from changes in protein conformations driven by Ca(2+) to the mechanisms that control Ca(2+) levels in the cytoplasm and organelles. Also discussed is the highly localized nature of Ca(2+)-mediated signal transduction and its specific roles in excitability, exocytosis, motility, apoptosis, and transcription.},
