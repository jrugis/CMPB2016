	Journal = {Biophys J},
	Journal-Full = {Biophysical journal},
	Mesh = {Algorithms; Biophysical Phenomena; Biophysics; Calcium Signaling; Computer Simulation; Inositol 1,4,5-Trisphosphate Receptors; Ion Channel Gating; Models, Biological; Patch-Clamp Techniques; Stochastic Processes},
	Month = {Sep},
