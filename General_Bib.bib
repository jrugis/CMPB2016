	Date-Modified = {2015-07-13 05:16:57 +0000},
	Journal = {J Physiol},
	Journal-Full = {The Journal of physiology},
	Mesh = {Animals; Calcium; Calcium Channels; Electrophysiology; In Vitro Techniques; Ion Channel Gating; Myocardial Contraction; Myocardium; Rats; Sarcoplasmic Reticulum},
