	Mesh = {Animals; Calcium; Calcium Channels; Gap Junctions; Male; Mesenteric Arteries; Models, Animal; Phenylephrine; Rats; Rats, Wistar; Vasoconstriction; Vasoconstrictor Agents},
	Month = {Feb},
