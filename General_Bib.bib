	Mesh = {Animals; Calcium Signaling; Cell Membrane; Cytosol; Humans; Ion Transport; Membrane Potentials; Mitochondria; Models, Biological; Myocytes, Smooth Muscle; Respiratory System; Signal Transduction},
	Number = {3},
	Pages = {285-302},
	Pmid = {17272854},
