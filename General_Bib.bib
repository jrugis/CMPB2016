	Mesh = {Action Potentials; Animals; Gonadotropin-Releasing Hormone; Hypothalamus; Mice; Mice, Transgenic; Models, Neurological; Neurons; Patch-Clamp Techniques; Receptors, AMPA; Receptors, GABA-A; Synapses; alpha-Amino-3-hydroxy-5-methyl-4-isoxazolepropionic Acid; gamma-Aminobutyric Acid},
	Month = {Jul},
	Number = {4},
