	Mesh = {Adaptation, Physiological; Animals; Calcium; Citric Acid Cycle; Computer Simulation; Energy Metabolism; Humans; Mitochondria, Heart; Models, Cardiovascular; Multienzyme Complexes; Oxidative Phosphorylation},
	Month = {Apr},
