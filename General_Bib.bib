	Journal-Full = {The Journal of physiology},
	Mesh = {Action Potentials; Animals; Arteries; Biological Clocks; Calcium Signaling; Humans; Models, Biological; Muscle Contraction; Muscle, Smooth, Vascular; Periodicity; Vasoconstriction},
