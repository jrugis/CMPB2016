	Journal-Full = {PLoS computational biology},
	Mesh = {Action Potentials; Algorithms; Animals; Computational Biology; Electrophysiological Phenomena; Glucose; Glycolysis; Humans; Insulin; Insulin-Secreting Cells; Mice; Models, Theoretical; Oscillometry; Pancreas; Potassium; Sodium Channels; Tetrodotoxin},
	Month = {Jan},
	Number = {1},
