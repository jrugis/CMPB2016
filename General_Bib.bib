@article{wasle2002,
	Abstract = {The pancreatic acinar cell synthesises a variety of digestive enzymes. In transit through the secretory pathway, these enzymes are separated from constitutively secreted proteins and packaged into zymogen granules, which are localised in the apical pole of the cell. Stimulation of the cell by secretagogues such as acetylcholine and cholecystokinin, acting at receptors on the basolateral plasma membrane, causes the generation of an intracellular Ca(2+) signal. This signal, in turn, triggers the fusion of the zymogen granules with the apical plasma membrane, leading to the polarised secretion of the enzymes. This review describes recent advances in our understanding of the control of secretion in the acinar cell. In particular, we discuss the mechanisms underlying the sorting of digestive enzymes into the zymogen granules, the molecular components of the exocytotic &quot;membrane fusion machine,&quot; the generation and propagation of the Ca(2+ signal and the development of new techniques for the visualisation of single granule fusion events.},
	Author = {W{\"a}sle, Barbara and Edwardson, J Michael},
	Date-Modified = {2013-06-11 09:32:04 +0000},
	Journal = {Cellular signalling},
	Number = {3},
	Pages = {191-197},
	Title = {The regulation of exocytosis in the pancreatic acinar cell},
	Volume = {14},
	Year = {2002}}

