@article{shuai2002,
	Abstract = {In many cell types, calcium is released from internal stores through calcium release channels upon external stimulation (e.g., pressure or receptor binding). These channels are clustered with a typical cluster size of about 20 channels, generating stochastic calcium puffs. We find that the clustering of the release channels in small clusters increases the sensitivity of the calcium response, allowing for coherent calcium responses at signals to which homogeneously distributed channels would not respond.},
