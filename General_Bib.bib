	Month = {Mar},
	Number = {1},
	Pages = {45-58},
	Pmid = {20005235},
	Pst = {ppublish},
	Title = {A model for {C}a$^{2+}$ waves in networks of glial cells incorporating both intercellular and extracellular communication pathways},
	Volume = {263},
