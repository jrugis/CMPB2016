	Journal-Full = {Philosophical transactions. Series A, Mathematical, physical, and engineering sciences},
	Mesh = {Animals; Biological Transport; Biophysics; Computer Simulation; Exocytosis; Glucose; Insulin; Insulin-Secreting Cells; Mice; Models, Biological; Models, Statistical; Models, Theoretical; Pancreas; Potassium; Time Factors},
	Month = {Oct},
	Number = {1880},
