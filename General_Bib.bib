	Mesh = {Animals; Calcium; Calcium Channels; Cell Membrane; Cytosol; Endoplasmic Reticulum; Feedback; GTP-Binding Proteins; Inositol 1,4,5-Trisphosphate; Inositol 1,4,5-Trisphosphate Receptors; Kinetics; Mathematics; Models, Biological; Receptors, Cell Surface; Receptors, Cytoplasmic and Nuclear; Type C Phospholipases},
	Month = {Mar},
	Number = {3},
