	Month = {Oct},
	Number = {Pt 5},
	Pages = {954-7},
	Pmid = {14505457},
	Pst = {ppublish},
	Title = {Modulation of {C}a$^{2+}$ oscillations by phosphorylation of {I}ns(1,4,5){P}$_3$ receptors},
