	Pages = {334-43},
	Pmc = {PMC1300641},
	Pmid = {10620297},
	Pst = {ppublish},
	Title = {Potentiation of fractional sarcoplasmic reticulum calcium release by total and free intra-sarcoplasmic reticulum calcium concentration},
	Volume = {78},
