	Mesh = {Animals; Biological Transport; Calcium; Cells; Cytosol; Exocytosis; Kinetics; Mice; Pancrelipase; Secretory Vesicles},
	Month = {Sep},
	Number = {Pt 18},
	Pages = {3201-8},
	Pmid = {20736314},
	Pst = {ppublish},
	Title = {Exocytosis, dependent on {C}a$^{2+}$ release from {C}a$^{2+}$ stores, is regulated by {C}a$^{2+}$ microdomains},
