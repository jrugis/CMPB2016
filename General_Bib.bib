	Mesh = {Animals; Axons; Biotin; Blood Vessels; Dendrites; GABA Agents; Galectin 1; Glutamic Acid; Gonadotropin-Releasing Hormone; Green Fluorescent Proteins; Male; Median Eminence; Membrane Potentials; Membrane Proteins; Mice; Mice, Inbred C57BL; Mice, Transgenic; Nerve Tissue Proteins; Neurons; Peptide Fragments; Photic Stimulation; Rhodopsin; Vesicular Inhibitory Amino Acid Transport Proteins; tau Proteins},
	Month = {Jul},
