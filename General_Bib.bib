	Journal = {Prog Biophys Mol Biol},
	Journal-Full = {Progress in biophysics and molecular biology},
	Mesh = {Acetylcholine; Animals; Calcium; Calcium Signaling; Humans; Inositol 1,4,5-Trisphosphate; Membrane Potentials; Mitochondria; Models, Biological; Muscle Cells; Respiratory System; Sarcoplasmic Reticulum},
	Number = {1-3},
