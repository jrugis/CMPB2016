	Journal-Full = {The Journal of general physiology},
	Mesh = {Action Potentials; Animals; Calcium; Cells, Cultured; Cytoplasm; Endoplasmic Reticulum; Islets of Langerhans; Male; Mice; Potassium Channels, Calcium-Activated},
	Month = {Sep},
	Number = {3},
