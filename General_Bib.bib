	Journal-Full = {The Journal of cell biology},
	Mesh = {Calcium; Cell Communication; Connexins; Glioma; Image Processing, Computer-Assisted; Intercellular Junctions; Membrane Proteins; Physical Stimulation; Transfection; Tumor Cells, Cultured},
