	Doi = {10.1074/jbc.M805865200},
	Journal = {J Biol Chem},
	Journal-Full = {The Journal of biological chemistry},
	Mesh = {Animals; Biological Clocks; Biosensing Techniques; COS Cells; Calcium; Calcium Signaling; Cercopithecus aethiops; Fluorescence Resonance Energy Transfer; Humans; Inositol 1,4,5-Trisphosphate},
	Month = {Mar},
